{
    "name": "pogbot",
    "version": "1.0.0",
    "description": "A Discord bot for useful gimmicks and fun",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint ./src/**/*",
        "format": "prettier --write ./",
        "compile": "tsc",
        "recompile": "npm run clean && npm run compile",
        "clean": "rm -r ./dist",
        "start": "node ./dist/src/index.js",
        "deployCommands": "node ./dist/src/deployCommands.js",
        "deploy": "npm run format && npm run lint && npm run recompile && npm run deployCommands && npm run start"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@discordjs/builders": "^0.6.0",
        "@discordjs/rest": "^0.1.0-canary.0",
        "@discordjs/voice": "^0.6.0",
        "discord-api-types": "^0.22.0",
        "discord.js": "^13.1.0",
        "dotenv": "^10.0.0"
    },
    "devDependencies": {
        "@types/node-fetch": "^2.5.12",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "eslint": "^7.32.0",
        "prettier": "^2.3.2"
    }
}
